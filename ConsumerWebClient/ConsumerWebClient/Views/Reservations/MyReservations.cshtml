@model IEnumerable<APIClient.DTOs.ReservationDTO>@{    ViewData["Title"] = "MyReservations";}<h1>Mine Reservationer</h1>@if (TempData["ProcessText"] != null) {    <div>@TempData["ProcessText"]</div>}<table class="table">    <thead>        <tr>            <th>                @Html.DisplayNameFor(model => model.ArrivalTime)            </th>            <th>                Antal jakker:            </th>            <th>                Antal tasker:            </th>            <th>                Pris:            </th>            <th></th>        </tr>    </thead>    <tbody>        @if (Model != null) {

            foreach (var item in Model) {                <tr>                    <td>                        @Html.DisplayFor(modelItem => item.ArrivalTime)                    </td>                    <td>                        @Html.DisplayFor(modelItem => item.AmountOfJackets)                    </td>                    <td>                        @Html.DisplayFor(modelItem => item.AmountOfBags)                    </td>                    <td>                        @Html.DisplayFor(modelItem => item.Price)                    </td>                </tr>            }
        }    </tbody></table>